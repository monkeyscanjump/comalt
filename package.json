{
  "name": "comalt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next",
    "dev": "next dev",
    "dev:prod": "npm run clean && cross-env NODE_ENV=production next dev",
    "dev:with-prod-data": "cross-env NEXT_PUBLIC_USE_PROD_DATA=true next dev",
    "build": "npm run clean && cross-env DISABLE_ESLINT_PLUGIN=true next build",
    "build:strict": "npm run clean && next build",
    "build:quick": "cross-env DISABLE_ESLINT_PLUGIN=true next build",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:setup": "npm run db:generate && npm run db:migrate:dev",
    "db:refresh": "npm run db:reset && npm run db:seed",
    "db:deploy": "npm run db:generate && npm run db:migrate:deploy"
  },
  "dependencies": {
    "@polkadot/api": "^9.0.0",
    "@polkadot/extension-dapp": "^0.58.5",
    "@polkadot/extension-inject": "^0.58.5",
    "@polkadot/util": "^13.4.3",
    "@polkadot/util-crypto": "^13.4.3",
    "@prisma/client": "^6.5.0",
    "@prisma/engines": "^6.5.0",
    "@subwallet/wallet-connect": "^0.2.7",
    "date-fns": "^4.1.0",
    "focus-visible": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.25",
    "next-navigation": "^1.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "rimraf": "^5.0.5",
    "systeminformation": "^5.25.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "path-browserify": "^1.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.5",
    "prisma": "^6.5.0",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "typescript": "~5.3.3"
  }
}
